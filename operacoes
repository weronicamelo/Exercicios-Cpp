#include <iostream>
using namespace std;

int main(){
    
    int valor1 = 10;
    int valor2 = 2;
    
    cout << valor1 + valor2 << endl;
    cout << valor1 - valor2 << endl;
    cout << valor1 * valor2 << endl;
    cout << valor1 / valor2 << endl;
    cout << valor1 % valor2 << endl;
    
    return 0;
}