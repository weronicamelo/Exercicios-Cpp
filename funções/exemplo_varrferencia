#include <iostream>
using namespace std;

void square(int &);

int main(){
    
    int number = 6;
    
    cout << "Antes da função: " << number << endl;
    square(number);
    cout << "Depois da função: " << number << endl;
    
    return 0;
}

void square(int &num){
    num = num * num;
    cout << "Durante a função: " << num << endl;
}