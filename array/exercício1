#include <iostream>
using namespace std;

int main() {
    float notas[5], soma = 0, media;

    cout << "Digite 5 notas: \n";
    for (int i = 0; i < 5; i++) {
        cout << "Nota " << i + 1 << ": ";
        cin >> notas[i];
        soma += notas[i]; 
    }
    
    media = soma / 5;

    cout << "\nNotas: \n";
    for (int i = 0; i < 5; i++) {
        cout << "Nota " << i + 1 << ": " << notas[i] << endl;
    }

    cout << "\nA média das notas é: " << media << endl;

    return 0;
}-