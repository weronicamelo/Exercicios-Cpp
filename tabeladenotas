#include <iostream>

using namespace std;

int main() {
    cout<<"Tabela de Notas: \n \n";
    cout<<"Aluno(a)     Nota \n";
    cout<<"=======      ==== \n";
    cout<<"Aluno 1:     9,0\n";
    cout<<"Aluno 2:     10,0\n";
    cout<<"Aluno 3:     4,5\n";
    cout<<"Aluno 4:     6,5\n";
    cout<<"Aluno 5:     7,0\n";

    return 0;
}