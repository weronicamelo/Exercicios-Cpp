#include <iostream>
using namespace std;

int main() {
    int numero;
    
    cout << "Digite o nÃºmero N do tabuleiro N x N: ";
    cin >> numero;
    
    for(int i = 1; i <= numero; i++){
        for(int k = 1; k <= numero; k++){
            cout << "-";
        }
        cout << endl;
    }
}